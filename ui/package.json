{
  "name": "ko-covid",
  "description": "KO Covid by Abhishek Das",
  "scripts": {
    "start": "webpack serve --config webpack.config.dev.js --port 3000",
    "clean:build": "rimraf ./build && mkdir build && copyfiles web.config ./build",
    "prebuild": "run-p clean:build",
    "build": "webpack --config webpack.config.prod.js",
    "postbuild": "run-p serve:build",
    "serve:build": "http-server ./build"
  },
  "dependencies": {
    "immer": "^9.0.2",
    "prop-types": "^15.7.2",
    "bootstrap": "^5.0.1",
    "bootswatch": "^5.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.4",
    "eslint": "^7.27.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "1.6.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "4.1.5",
    "postcss-loader": "5.3.0",
    "rimraf": "3.0.2",
    "style-loader": "2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
